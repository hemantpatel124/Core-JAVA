import java.util.Scanner;

public class Loops {
	public static void main(String[] args) {
		//for loop
		Scanner sc= new Scanner(System.in);
		int i;
		//int start=sc.nextInt();
		System.out.println("enter end valu:");
		int end=sc.nextInt();
		for(i=0;i<=end; i++) {
			System.out.println("Hemant");
		}
	}
}
