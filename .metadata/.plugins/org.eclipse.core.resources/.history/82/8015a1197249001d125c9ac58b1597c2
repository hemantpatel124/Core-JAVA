public class A5 {
	public static void main(String[] args) {
	
		int num[]=new int[5];
		Scanner scanner=new Scanner(System.in);
		
		for(int i=0;i<=num.length; i++) {
			
			System.out.println("Enter num"+num[i]+" : ");
			
		}
	
	}

}
