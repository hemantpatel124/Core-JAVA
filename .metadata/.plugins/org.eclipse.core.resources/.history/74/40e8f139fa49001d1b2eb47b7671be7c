/* "Write a Java program that accepts an integer (n) and computes the value  of n+nn+nnn. 
Input number: 5
5 + 55 + 555"
 */
package assignment;

public class A11 {
	
	public static void main(String[] args) {
		
		long f=0,ans=0;
		
		for(int i=1;i<=3;i++) {
			f=f*10+5;
			ans+=f;
		}
		System.out.println("Compute value : "+ans);
	}
}
